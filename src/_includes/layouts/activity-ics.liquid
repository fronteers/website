BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//Fronteers//Event Calendar//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
BEGIN:VEVENT
UID:{{ pagination.items[0].data.key | default: pagination.items[0].fileSlug }}@fronteers.nl
DTSTAMP:{{ "now" | date: "%Y%m%dT%H%M%SZ" }}
DTSTART{% if pagination.items[0].data.eventtime %}:{{ pagination.items[0].data.eventdate | icsDateTime: pagination.items[0].data.eventtime }}{% else %};VALUE=DATE:{{ pagination.items[0].data.eventdate | date: "%Y%m%d" }}{% endif %}
{% if pagination.items[0].data.eventtime %}DTEND:{{ pagination.items[0].data.eventdate | icsEndDateTime: pagination.items[0].data.eventtime }}{% endif %}
SUMMARY:{{ pagination.items[0].data.title | icsEscape }}
{% if pagination.items[0].data.summary %}DESCRIPTION:{{ pagination.items[0].data.summary | icsEscape }}{% endif %}
{% if pagination.items[0].data.location %}LOCATION:{{ pagination.items[0].data.location | icsEscape }}{% endif %}
{% if pagination.items[0].url %}URL:{{ site.baseUrl }}{{ pagination.items[0].url }}{% endif %}
STATUS:CONFIRMED
SEQUENCE:0
END:VEVENT
END:VCALENDAR
